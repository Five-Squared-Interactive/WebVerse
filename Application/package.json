  "devDependencies": {
    "electron": "^26.1.0",
    "electron-builder": "^24.6.4"
  },
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.fivesqd.webverse",
    "icon": "images/WebVerse.png",
